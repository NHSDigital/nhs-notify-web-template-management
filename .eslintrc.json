{
  "extends": [
    "next",
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["prettier", "jsx-a11y", "unused-imports"],
  "rules": {
    "prettier/prettier": "error",
    "react/display-name": "off",
    "unused-imports/no-unused-imports": "error"
  }
}
