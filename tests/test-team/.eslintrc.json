{
  "extends": [
    "../../.eslintrc",
    "plugin:playwright/recommended",
    "plugin:jest/recommended"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**"
        ]
      }
    ],
    "playwright/expect-expect": "off",
    "playwright/no-conditional-in-test": "off",
    "playwright/no-conditional-expect": "off"
  }
}