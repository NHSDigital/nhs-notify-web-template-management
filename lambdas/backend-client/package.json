    "axios": "^1.7.8"
  },
  "devDependencies": {
    "@swc/core": "^1.9.2",
    "@swc/jest": "^0.2.37",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.14",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^29.7.0",
    "esbuild": "^0.24.0",
    "jest-mock-extended": "^3.0.7",
    "typescript": "^5.5.4"
  }
}