{
  "name": "nhs-notify-backend-client",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outdir=.build --format=cjs --platform=node",
